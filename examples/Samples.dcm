# How to define an array consructor in dcm

[ none copy dup $delim ] def $(

[
    [ @delim ] 
    [ "No delimiter defined, missing '(' ?" error ] catch
    : {
        $delim
        0  $len
        [dup delim is not]
        [
            @len 1 + $len
            [ $elems ]
            [ "No delimiter found, corruption?" error ] catch
        ]
        while
        del
        # We've gotten everything
        
        len array
        @len $i
        [i 0 >]
        [
            @i 1 - $i
            dup i @elems (=)
        ]
        while
        # We've filled the array
    } del
] def $)

